{
  "title": "wap",
  "description": "wap",
  "memory_usage": 80,
  "licenses": [
  ],
  "binding_anchor": {
    "variables": {
      "fqdn": "www.mydomain.com",
      "protocol": "https",
      "port": "8000",
      "directory": "/",
      "public": true,
      "engine_count": "1",
      "system_auth": false,
      "auth_group": "^^identifier^^",
      "require_client_ssl": false
    }
  },
  "user": {
    "identifier": "22005",
    "name": "www-data",
    "create": false
  },
  "stages": [
    {
      "starter": {
        "image": "engines/servicebase",
        "tag": "current"
      },
      "system_packages": [
        {
          "package": "nginx"
        }
      ],
      "ports": [
        {
          "port": "8000",
          "external": "80",
          "protocol": "tcp"
        },
        {
          "port": "4430",
          "external": "443",
          "protocol": "tcp"
        }
      ]
    }
  ]
}
